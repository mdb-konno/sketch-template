  "scripts": {
    "build": "npm run build:sketchtool & npm run build:json",
    "build:json": "bash ./sh/sketch-to-json.sh template",
    "build:sketchtool": "sketchtool export artboards ./sketch/$npm_package_config_fileName.sketch --output=./dist/images --outputJSON=./json/sketchtool.json --format=png",
    "get": "sketchtool list artboards ./sketch/$npm_package_config_fileName.sketch",
    "start": "npm run watch",
    "watch": "watch 'npm run build & npm run watch:msg' ./sketch",
