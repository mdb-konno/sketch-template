{
  "name": "sketch-template",
  "description": "cool sketch-template",
  "version": "0.1.0",
  "main": "template.sketchplugin",
  "manifest": "src/manifest.json",
  "config": {
    "distPath": "./dist/images/",
    "fileName": "template",
    "format": "png",
    "srcPath": "./src/sketch/"
  },
  "scripts": {
    "skt:exp": "sketchtool export artboards $npm_package_config_fileName --output=$npm_package_config_distPath --format=$npm_package_config_format",
    "skt:list": "sketchtool list artboards $npm_package_config_fileName",
  },
  "dependencies": {},
  "devDependencies": {},
  "author": "@mdb-konno",
  "bugs": {
    "url": "https://github.com/mdb-konno/sketch-template/issues"
  },
  "homepage": "https://github.com/mdb-konno/sketch-template#readme",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mdb-konno/sketch-template.git"
  }
}
